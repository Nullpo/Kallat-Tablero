<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.kallat" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Kallat - Tablero Virtual</name>
    <description>
        Tablero virtual para gestionar las estad√≠sticas del Invocador en kallat.
    </description>
    <author email="pablocodeiro@gmail.com" href="http://phonegap.com">
        Pablo Codeiro
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="windows-appx-target" value="uap" />
    <preference name="permissions" value="none" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
